{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "include": ["src/**/*"],
  "baseUrl": "src",
  "exclude": ["**/node_modules"],
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
